{
  "name": "@B-S-F/frog-vue",
  "version": "2.10.2",
  "repository": "https://github.com/B-S-F/yaku/yaku-ui/frog-vue.git",
  "description": "FroK components build with Vue 3",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist",
    "src",
    "vite.config.ts",
    "tsconfig*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "concurrently \"npm:type-check\" \"npm:build-only\"",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "release": "release-it",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:unit:run": "vitest run --passWithNoTests --environment jsdom --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "release-it-patch": "release-it patch -n --verbose",
    "release-it-minor": "release-it minor -n --verbose",
    "release-it-major": "release-it major -n --verbose",
    "story": "histoire dev",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.4",
    "vite-plugin-vuetify": "^1.0.2",
    "vuetify": "^3.7.2",
    "@mdi/font": "^7.4.47"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "^0.17.17",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "histoire": "^0.17.17",
    "jsdom": "^20.0.3",
    "release-it": "^16.2.1",
    "sass": "^1.69.4",
    "typescript": "^4.9.4",
    "vite": "^4.5.5",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.25.6",
    "vue": "^3.5.11",
    "vue-tsc": "^1.0.12"
  },
  "peerDependencies": {
    "@vitejs/plugin-vue": "~4 || ~5",
    "@vueuse/core": ">=9",
    "vue": ">= 3.4"
  }
}
