{
  "name": "@B-S-F/qg-service-ui",
  "repository": "https://github.com/B-S-F/yaku/yaku-ui/qg-service-ui.git",
  "private": true,
  "version": "1.20.0",
  "type": "module",
  "scripts": {
    "start": "serve -s dist",
    "dev": "vite --host",
    "dev:mock": "vite --host --mode dev:mock --port 5173",
    "dev:test": "vite --mode dev:mock --port 4999",
    "check": "vue-tsc --noEmit",
    "build": "vite build",
    "docker:build": "docker-buildx build . -t qg-aas-ui:latest",
    "docker:start": "docker run -it --mount=type=bind,src=./ui-dev-config.json,dst=/home/node/app/dist/ui-config.json,readonly=true -p 3000:3000 qg-aas-ui:latest",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "preview": "vite preview",
    "release-it-patch": "release-it patch -n --verbose",
    "release-it-minor": "release-it minor -n --verbose",
    "release-it-major": "release-it major -n --verbose",
    "test:build": "node --stack_size=8192 ./node_modules/vite/bin/vite.js build --mode dev:mock && mv dist/index.dev.html dist/index.html",
    "test:preview": "vite preview --port 4999",
    "test:unit": "TZ=UTC LC_ALL=de_DE.UTF-8 vitest",
    "test:unit:run": "TZ=UTC LC_ALL=de_DE.UTF-8 vitest run --coverage.enabled --coverage.reporter='text'",
    "test:unit:ci": "TZ=UTC LC_ALL=de_DE.UTF-8 vitest run --coverage.enabled --coverage.reporter='text-summary' --coverage.reporter='cobertura'",
    "test:e2e": "start-server-and-test dev:test http://localhost:4999 cy",
    "test:e2e:report": "REPORTER=ok CYPRESS_COVERAGE=true start-server-and-test dev:test http://localhost:4999 cy ; node cypress/cleanupReport.js",
    "cy": "cypress run --browser chrome",
    "cy:update": "cypress run --browser chrome --env pluginVisualRegressionUpdateImages=true",
    "cy:open": "cypress open"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.19",
    "@frsource/cypress-plugin-visual-regression-diff": "^3.3.10",
    "@mswjs/data": "^0.11.2",
    "@testing-library/vue": "^6.6.1",
    "@types/dompurify": "^3.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/jsdom": "^21.1.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitest/coverage-v8": "^1.0.1",
    "@vitest/ui": "^1.0.1",
    "@vue/eslint-config-typescript": "^11.0.3",
    "ansicolor": "^1.1.100",
    "cross-fetch": "^3.1.5",
    "cypress": "^13.2.0",
    "cypress-mochawesome-reporter": "^3.8.0",
    "cypress-multi-reporters": "^1.6.4",
    "eslint": "^8.49.0",
    "jsdom": "^22.1.0",
    "mocha-junit-reporter": "^2.2.1",
    "msw": "^2.7.0",
    "release-it": "^17.0.3",
    "sass": "^1.66.1",
    "start-server-and-test": "^2.0.5",
    "typescript": "^4.9.4",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.4.7",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^1.1.1",
    "vue": "3.5.11",
    "vue-tsc": "^1.6.5"
  },
  "dependencies": {
    "@azure/msal-browser": "3.5",
    "@B-S-F/frog-vue": "^2.10.2",
    "@vueuse/components": "^10.7.2",
    "@vueuse/core": "^10.7.2",
    "chart.js": "^4.4.0",
    "chartjs-adapter-spacetime": "^1.0.1",
    "dompurify": "^3.1.3",
    "js-yaml": "^4.1.0",
    "keycloak-js": "^23.0.7",
    "markdown-it": "^13.0.1",
    "monaco-editor": "^0.39.0",
    "monaco-yaml": "^5.1.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "pinia-shared-state": "^0.4.5",
    "serve": "^14.2.0",
    "smartquotes": "^2.3.2",
    "spacetime": "^7.4.8",
    "vue-chartjs": "^5.2.0",
    "vue-markdown-render": "^2.2.1",
    "vue-router": "^4.2.4",
    "vuetify": "^3.7.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.1/xlsx-0.20.1.tgz"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
