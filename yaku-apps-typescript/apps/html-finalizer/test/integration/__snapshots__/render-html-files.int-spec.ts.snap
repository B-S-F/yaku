page-break-inside: avoid;
}

body {
margin: 2cm;
padding: 0;
}

p, ul {
margin: 0;
}

p+p, ul+p, p+ul {
margin-top: 1em;
}

.small, .sources {
font-size: .75rem;
}

.normal {
font-size: 1rem;
}

.centered {
text-align: center;
}

.nospace {
position: absolute;
z-index: 1;
transform: translateX(.25rem);
}

.full-width {
width: 100%;
}

.icon {
width: 1.5rem;
height: 1.5rem;
margin-top: 0;
margin-bottom: -0.5rem;
vertical-align: top;
}

.icon[data-icon^="text-"] {
width: 3rem;
}

h3 .icon[data-icon^="text-"] {
display: none;
}

ul.sources {
list-style-type: none;
padding: 0;
margin-top: 0;
}

h2, h3 {
margin: 1em 0;
}
h3 {
font-size: 1.25rem;
}

h3 .status-icon {
float: right;
font-weight: normal;
font-size: 1rem;
white-space: nowrap;
margin-left: .25rem;
}

.capture-filter-box {
position: fixed;
display: none;
z-index: 1001;
top: 0;
left: 0;
width: 100%;
height: 100%;
}

.logs-header {
background: #999;
display: block;
color: white;
text-align: center;
padding: 0.5em 1em;
margin-bottom: 0px;
font-weight: bold;
border-radius: 0.5em 0.5em 0em 0em;
font-size: 0.7rem;
}

.error-logs {
background: rgb(119, 14, 14);
}

pre.logs {
background: #333;
font-size: 0.7rem;
color: white;
padding: 1em;
margin-top: 0;
white-space: pre-wrap;
overflow-wrap: break-word;
border-radius: 0em 0em 0.5em 0.5em;
}

.toggle-hidden::before {
content: "▶ "
}

.toggle-hidden::after {
content: " (click to expand)"
}

.toggle-expanded::before {
content: "▼ "
}

.toggle-expanded::after {
content: " (click to minimize) "
}

.hidden {
display: none;
}

.legend {
display: grid;
grid-template-columns: min-content 1fr;
grid-auto-rows: 1.5rem;
gap: .25rem .5rem;
vertical-align: bottom;
margin-top: 2rem;
page-break-inside: avoid;
}

.legend .icon {
margin: 0;
}

.legend > span {
justify-self: center;
}

.legend > * {
align-self: center;
}

.marked table, .marked thead {
background-color: var(--gray-95);
}

/* === Bar chart === */

.bar-chart {
display: flex;
align-content: stretch;
}

.bar-chart > * + * {
outline: 2px solid #fff;
background-color: var(--gray-60);
height: 1.5rem;
}

.bar-chart-legend:first-of-type {
margin-left: .5rem;
}

.bar-chart-legend {
white-space: nowrap;
margin-right: .5rem;
}

.bar-chart-legend__symbol {
display: inline-block;
width: .75rem;
height: .75rem;
border-radius: .375rem;
}

[data=auto] {
background-color: var(--turquoise-30);
}

[data=manual] {
background-color: var(--turquoise-50);
}

[data=unanswered] {
background-color: var(--turquoise-70);
}

[data=GREEN] {
background-color: var(--green-50);
}

[data=YELLOW] {
background-color: var(--yellow-85);
}

[data=RED] {
background-color: var(--red-50);
}

[data=NA] {
background-color: var(--blue-50);
}

[data=empty] {
background-color: var(--gray-95);
}

@media print {
:root {
font-size: 10pt;
}

body,
html {
margin: 0 2cm;
}

.no-print,
.no-print * {
display: none !important;
}
}

@page {
margin: 2cm 0;
font-family: Ubuntu, Helvetica, Arial, sans-serif;
}
