{
  "name": "@B-S-F/gh-app",
  "version": "0.3.1",
  "description": "",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "nodemon --watch \"src/**\" --exec npm run start",
    "start": "node ./dist/index.js",
    "lint": "eslint '**/*.ts'",
    "format": "prettier --write '**/*.{ts,md}'",
    "test": "vitest run && npm run test:update-cobertura-file",
    "test:update-cobertura-file": "sed -i'.bak' 's,package name=\",package name=\"'${npm_package_name}/',g' coverage/cobertura-coverage.xml",
    "test:dev": "vitest -w",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [],
  "author": "",
  "license": "",
  "devDependencies": {
    "@B-S-F/eslint-config": "*",
    "@B-S-F/typescript-config": "*",
    "@types/jsonpath": "^0.2.0",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@vitest/coverage-v8": "*",
    "c8": "*",
    "eslint": "*",
    "eslint-config-prettier": "*",
    "prettier": "*",
    "tsup": "*",
    "typescript": "*",
    "vitest": "*"
  },
  "dependencies": {
    "@B-S-F/autopilot-utils": "^0.11.0",
    "https-proxy-agent": "^7.0.4",
    "octokit": "^3.1.2",
    "undici": "^6.19.2",
    "universal-github-app-jwt": "^2.2.0"
  },
  "bin": {
    "gh-app": "dist/index.js"
  }
}
